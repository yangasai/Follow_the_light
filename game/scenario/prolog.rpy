    $ Fl.PlaySound("Fl_open_cupboard", 1, 0, False)
    $ Fl.Pause (2.0)
    $ Fl.ShowScreens(_with=Fl_fast)
    $ Fl.say(Fl_th, "Нашёл!")
    $ Fl.Item("headphones")
    $ Fl.Pause (1.0)
    $ Fl.say(Fl_r, "Наушники лежали сверху различного барахла, которго скопилось немало за пару месяцев.")
    $ Fl.HideScreens(_with=Fl_dissolve1)
    $ it_0 += 1
    $ hp += 1
    if hp >=1 and ph >=1:
        jump Fl_prologue_cnt
    else:
